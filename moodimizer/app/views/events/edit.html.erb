<h1>Events#edit</h1>
<p>Find me in app/views/events/edit.html.erb</p>

<%= form_for @event do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :url %>
	<%= f.text_area :url, :style => "width: 300px; height: 50px;" %>
  <%= f.submit "Update event", class: "btn btn-large btn-info" %>
<% end %>

<%= link_to 'Cancel', user_path %>
